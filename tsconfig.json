{
	"compilerOptions": {
		"strict": true,
		"allowSyntheticDefaultImports": true,
		"noStrictGenericChecks": true,
		"target": "es2017",
		"module": "commonjs",
		"moduleResolution": "node",
		"esModuleInterop": false,
		"experimentalDecorators": true,
		"lib": [
			"es2015",
			"es2016",
			"es2017",
			"es2018",
			"es2019",
			"es2020"
		]
	},
	"exclude": [
		"random"
	]
}
